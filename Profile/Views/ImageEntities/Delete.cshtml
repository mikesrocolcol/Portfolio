@model Profile.Models.ImageEntity

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_mainGallery.cshtml";
}
<div class="top">
    <h1>Delete</h1>


    <h3>Are you sure you want to delete this?</h3>
</div>



<div class=".container-sm table-create">
    <div class="col-md-4 ">
        <dl class="row">


            <div class="col-auto">
                <img src="@("~/images/"+Model.ImageFile)" asp-append-version="true" width="300" height="300" />

            </div>




        </dl>
        <div class="form-group">
            <form asp-action="Delete">
                <input type="hidden" asp-for="Id" />
                <input type="submit" value="Delete" class="btn btn-danger" style="width:150px" />
                <a asp-action="Index" class="btn btn-secondary" style="width:150px">Back to List</a>
            </form>
        </div>
    </div>
</div>
